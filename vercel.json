{
  "version": 2,
  "name": "eat-fine",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "SESSION_SECRET": "@session_secret",
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 60
    }
  }
}